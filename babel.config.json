{
  "presets": ["@babel/preset-env"],
  "plugins": [
    [
      "babel-plugin-root-import", {
        "paths": [
          {
            "rootPathSuffix": "./src",
            "rootPathPrefix": "~/"
          },
          {
            "rootPathSuffix": "./src/components",
            "rootPathPrefix": "@"
          }
        ]
      }
    ],
    ["@babel/plugin-transform-runtime"]
  ]
}